{
	"name":"dateCompare",
	"type":"function",
	"syntax":"dateCompare(date1, date2 [, datePart])",
	"returns":"Numeric",
	"related":[],
	"description":" Performs a full date/time comparison of two dates.\n -1, if date1 is less than date2\n 0, if date1 is equal to date2\n 1, if date1 is greater than date2\n [datepart - quicky]\n yyyy: Year; q: Quarter; m: Month; y: Day of year; d: Day\n w: Weekday; ww: Week; h: Hour; n: Minute; s: Second;\n l: Millisecond",
	"params": [
		{"name":"date1","description":"","required":true,"default":"","type":"DateTime","values":["now()"]},
		{"name":"date2","description":"","required":true,"default":"","type":"DateTime","values":["now()"]},
		{"name":"datePart","description":"","required":false,"default":"","type":"String","values":["yyyy","q","m","y","d","w","ww","h","n","s","l"]}

	],
